export CLICOLOR=1
# colors
BLACK='\033[0;30m'
BLUE='\033[0;34m'
BROWN='\033[0;33m'
CYAN='\033[0;36m'
DARK_GRAY='\033[1;30m'
GREEN='\033[0;32m'
PURPLE='\033[0;35m'
RED='\033[0;31m'
WHITE='\033[1;37m'
YELLOW='\033[1;33m'
# Light colors
LIGHT_BLUE='\033[1;34m'
LIGHT_CYAN='\033[1;36m'
LIGHT_GRAY='\033[0;37m'
LIGHT_GREEN='\033[1;32m'
LIGHT_PURPLE='\033[1;35m'
LIGHT_RED='\033[1;31m'
# White on X
WHITE_ON_BLUE="\033[44;1;37m";
WHITE_ON_GREEN="\033[42;1;37m";
WHITE_ON_RED="\033[41;1;37m";
# bold
BLUE_BOLD='\033[01;34m'
CYAN_BOLD='\033[01;36m'
GREEN_BOLD='\033[01;32m'
PURPLE_BOLD='\033[01;35m'
RED_BOLD='\033[01;31m'
# no color
NC='\033[0m'
