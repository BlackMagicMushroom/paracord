$ECHO -e "${BLUE_BOLD} __________                                            .___ "
$ECHO -e "${BLUE_BOLD} \______   \_____ ____________    ____  ___________  __| _/ ${NC}"
$ECHO -e "  |     ___/\__  \\\\\_  __ \__  \ _/ ___\/  _ \_  __ \/ __ |  "
$ECHO -e "  |    |     / __ \|  | \// __ \\\\\  \__(  <_> )  | \/ /_/ | "
$ECHO -e " ${BLUE} |____|    (____  /__|  (____  /\___  >____/|__|  \____ |  "
$ECHO -e " ${BLUE}                \/           \/     \/                 \/  ${NC}"
$ECHO -e ""
