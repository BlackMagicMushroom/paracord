$ECHO -e "${BLUE} __________                                            .___ "
$ECHO -e "${BLUE} \______   \_____ ____________    ____  ___________  __| _/ ${NC}"
$ECHO -e "${BLUE_BOLD}  |     ___/\__  \\\\\_  __ \__  \ _/ ___\/  _ \_  __ \/ __ |  "
$ECHO -e "${BLUE_BOLD}  |    |     / __ \|  | \// __ \\\\\  \__(  <_> )  | \/ /_/ | ${NC}"
$ECHO -e "  |____|    (____  /__|  (____  /\___  >____/|__|  \____ |  "
