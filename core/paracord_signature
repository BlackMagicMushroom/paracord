$ECHO -e "${BLUE} __________                                            .___ ${NC}"
$ECHO -e "${BLUE} \______   \_____ ____________    ____  ___________  __| _/ ${NC}"
$ECHO -e "  |     ___/\__  \\\\\_  __ \__  \ _/ ___\/  _ \_  __ \/ __ |  "
$ECHO -e "  |    |     / __ \|  | \// __ \\\\\  \__(  <_> )  | \/ /_/ |  "
$ECHO -e "${BLUE}  |____|    (____  /__|  (____  /\___  >____/|__|  \____ |  ${NC}"
$ECHO -e "${BLUE}                 \/           \/     \/                 \/  ${NC}"

